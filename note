Labyrinth generated with JavaScript:<br><br>
<script>
function dd(a,b,c,d){

function permute(input) {
    var permArr = [],
        usedChars = [];
    return (function main() {
        for (var i = 0; i < input.length; i++) {
            var ch = input.splice(i, 1)[0];
            usedChars.push(ch);
            if (input.length == 0) {
                permArr.push(usedChars.slice());
            }
            main();
            input.splice(i, 0, ch);
            usedChars.pop();
        }
        return permArr;
    })();
}


function makeNumber(a){
	return a[0]*1000+a[1]*100+a[2]+a[3];
}
function getBigger(a,b) {
	if (makeNumber(a) >makeNumber(b)){
    	return a;
    }
    else{
    	return b;
    }
}

function isHour(input){
	var returnMe = true;
    if (input[0] > 2) {
    	returnMe = false;
    }
    else if ( input[0] == 2) {
		    if( input[1] > 4) {
            	returnMe = false;
            }
    }
    if (input[2] > 5) {
    	returnMe = false;
    }
    return returnMe;
}

var list = permute([a,b,c,d]);
var maxHour =[0,0,0,0];

list.forEach(function(entry){
	document.write(entry);
    document.write(" | " + isHour(entry));
    document.write("<br>");
    if(isHour(entry)){
    	maxHour=getBigger(maxHour,entry);
    }
}

);

document.write(maxHour);

}
dd(1,2,6,8)

</script>
